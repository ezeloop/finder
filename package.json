{
  "name": "finder-server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/nest start",
    "start:dev": "cross-env NODE_ENV=development ./node_modules/.bin/nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "-r tsconfig-paths/register ./node_modules/typeorm/cli.js --config orm.config.ts",
    "typeorm:generate": "ts-node node_modules/typeorm/cli.js migration:generate --config orm.config.ts --name",
    "typeorm:migrate": "./node_modules/.bin/ts-node node_modules/typeorm/cli.js migration:run --config orm.config.ts",
    "typeorm:show": "ts-node node_modules/typeorm/cli.js migration:show --config orm.config.ts",
    "typeorm:revert": "ts-node node_modules/typeorm/cli.js migration:revert --config orm.config.ts"
  },
  "dependencies": {
    "@apollo/gateway": "^0.48.3",
    "@nestjs/apollo": "^10.0.7",
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/graphql": "^10.0.7",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/terminus": "^7.1.0",
    "@nestjs/typeorm": "^8.0.3",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "apollo-server-core": "^3.6.4",
    "apollo-server-express": "^3.6.4",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "graphql": "^16.3.0",
    "helmet": "^4.4.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "postgres": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.1.1",
    "swagger-ui-express": "^4.1.6",
    "ts-morph": "^14.0.0",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@nestjs/cli": "^7.5.6",
    "@nestjs/schematics": "^7.2.7",
    "@nestjs/testing": "^7.6.13",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "cls-rtracer": "2.6.0",
    "concurrently": "^6.1.0",
    "cross-env": "^7.0.3",
    "dot-plop": "^0.4.1",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "npm-force-resolutions": "0.0.10",
    "plop": "^2.7.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sonarqube-scanner": "^2.8.0",
    "standard-version": "^9.1.1",
    "stubmatic": "^5.4.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
